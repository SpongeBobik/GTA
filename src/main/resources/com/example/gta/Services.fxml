<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gta.ServicesController">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="1000.0" styleClass="background1" stylesheets="@main.css">
         <children>
            <TableView fx:id="tableServices" layoutX="112.0" layoutY="90.0" onMouseClicked="#getcell" prefHeight="241.0" prefWidth="600.0" stylesheets="@main.css" />
            <Pane layoutX="858.0" layoutY="-1.0" prefHeight="600.0" prefWidth="145.0" style="-fx-background-color: black;">
               <children>
                  <Button fx:id="bttAddServ" layoutX="10.0" layoutY="81.0" mnemonicParsing="false" onAction="#addService" text="Добавить" textFill="#b3f3f5">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="bttDeleteServ" layoutX="22.0" layoutY="123.0" mnemonicParsing="false" onAction="#DeleteService" text="Удалить" textFill="#b3f3f5">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="bttChangeServ" layoutX="17.0" layoutY="165.0" mnemonicParsing="false" text="Изменить" textFill="#b3f3f5">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="bttServBack" layoutX="23.0" layoutY="533.0" mnemonicParsing="false" onAction="#Back" prefHeight="44.0" prefWidth="106.0" text="Назад" textFill="#b3f3f5">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="UpdateSale" layoutX="17.0" layoutY="249.0" mnemonicParsing="false" onAction="#UpdateServiceTable" text="Обновить" textFill="#b1fff6e5">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Button>
               </children>
            </Pane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
