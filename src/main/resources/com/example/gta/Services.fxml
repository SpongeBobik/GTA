<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gta.ServicesController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="background1" stylesheets="@main.css">
         <children>
            <TableView fx:id="tableServices" layoutX="50.0" layoutY="46.0" onMouseClicked="#getcell" prefHeight="241.0" prefWidth="382.0" stylesheets="@main.css" />
            <Pane layoutX="490.0" prefHeight="400.0" prefWidth="110.0" style="-fx-background-color: black;">
               <children>
                  <Button fx:id="bttAddServ" layoutX="15.0" layoutY="82.0" mnemonicParsing="false" text="Добавить" textFill="#9df8ff">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Button>
                  <Button fx:id="bttDeleteServ" layoutX="19.0" layoutY="118.0" mnemonicParsing="false" text="Удалить" textFill="#9df8ff">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Button>
                  <Button fx:id="bttChangeServ" layoutX="14.0" layoutY="151.0" mnemonicParsing="false" text="Изменить" textFill="#9df8ff">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Button>
                  <Button fx:id="bttServBack" layoutX="15.0" layoutY="359.0" mnemonicParsing="false" onAction="#Back" prefHeight="25.0" prefWidth="81.0" text="Назад" textFill="#9df8ff">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Button>
               </children>
            </Pane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
